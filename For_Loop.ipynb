    "# A for loop is used when you know how many times you want to repeat.\n",
    "\n",
    "#Example: loop through a list\n",
    "fruits = [\"apple\", \"banana\", \"mango\"]\n",
    "for fruit in fruits:\n",
    "    print(fruit)\n",
    "\n",
    "#Example: loop using range()\n",
    "for i in range(5):\n",
    "    print(i)"
